{
  "v2": {
    "transposit": {
      "dependencies": [{
        "name": "transposit/slack:v2.*.*",
        "alias": "slack"
      }, {
        "name": "transposit/slack_webhook:v1.*.*",
        "alias": "slack_webhook"
      }, {
        "name": "transposit/aws_athena:v1.*.*",
        "alias": "aws_athena"
      }, {
        "name": "transposit/google_sheets:v1.*.*",
        "alias": "google_sheets"
      }, {
        "name": "transposit/google_mail:v4.*.*",
        "alias": "google_mail"
      }],
      "operations": [{
        "id": "webhook",
        "function": {
          "parameters": [{
            "name": "http_event",
            "default": {
              "text": "{   \"query_parameters\": {     \"api_key\": \"XXX\"   },   \"http_method\": \"POST\",   \"parsed_body\": {     \"channel_name\": \"XXX\",     \"user_id\": \"UXXX\",     \"user_name\": \"iggy\",     \"trigger_id\": \"XXX\",     \"team_domain\": \"my_domain\",     \"team_id\": \"TXXX\",     \"text\": \"\",     \"channel_id\": \"DXXX\",     \"command\": \"/test\",     \"token\": \"XXX\",     \"response_url\": \"https://hooks.slack.com/commands/TXXX/XXX/XXX\"   },   \"body\": \"token=XXX&team_id=TXXX&team_domain=my_domain&channel_id=DXXX&channel_name=XXX&user_id=UXXX&user_name=iggy&command=%2Ftest&text=&response_url=https%3A%2F%2Fhooks.slack.com%2Fcommands%2FTXXX%2FXXX%2FXXX&trigger_id=XXX\" }"
            },
            "type": "OBJECT"
          }],
          "js": "webhook.js"
        },
        "access": {
          "requireApiKey": true,
          "webhook": true
        }
      }, {
        "id": "get_slack_message",
        "function": {
          "parameters": [{
            "name": "text",
            "type": "STRING"
          }],
          "js": "get_slack_message.js"
        }
      }, {
        "id": "test_post_message",
        "tql": {
          "tql": "-- This SQL operation is useful to test the message you've\n-- constructed for your webhook. If you provide a Slack\n-- auth in \"Auth & user settings\", it will post a message\n-- to your Slackbot channel.\nSELECT * FROM slack.post_chat_message AS A\nJOIN this.get_slack_message AS B\nON A.$body.blocks = B.blocks\nAND A.$body.text = B.text\nWHERE A.$body.channel = (SELECT user_id from slack.test_auth)"
        }
      }, {
        "id": "query_athena",
        "function": {
          "parameters": [{
            "name": "query",
            "type": "STRING"
          }, {
            "name": "response_url",
            "type": "STRING"
          }],
          "js": "query_athena.js"
        }
      }, {
        "id": "get_sheet_values",
        "tql": {
          "parameters": [{
            "name": "sheetId",
            "type": "STRING"
          }],
          "tql": "SELECT * FROM google_sheets.get_sheet_values\n  WHERE range='A1:D4'\n  AND spreadsheetId=@sheetId\n\n/*\n * For sample code and reference material, visit\n * https://www.transposit.com/docs/get-started/sql-quickstart\n */"
        }
      }, {
        "id": "get_metrics_data_and_post",
        "function": {
          "js": "untitled_1.js"
        }
      }, {
        "id": "post_to_slack",
        "function": {
          "parameters": [{
            "name": "sections",
            "type": "ARRAY"
          }],
          "js": "post_to_slack.js"
        }
      }, {
        "id": "process_athena_query",
        "function": {
          "parameters": [{
            "name": "stashId",
            "type": "STRING",
            "required": true
          }],
          "js": "process_athena_query.js"
        }
      }, {
        "id": "check_status",
        "function": {
          "parameters": [{
            "name": "stashId",
            "type": "STRING",
            "required": true
          }],
          "js": "check_status.js"
        }
      }, {
        "id": "delete_all_tasks",
        "function": {
          "js": "delete_all_tasks.js"
        },
        "access": {
          "requireApiKey": true
        }
      }, {
        "id": "delete_all_stash",
        "function": {
          "js": "delete_all_stash.js"
        },
        "access": {
          "requireApiKey": true
        }
      }, {
        "id": "get_query_results",
        "tql": {
          "parameters": [{
            "name": "id",
            "type": "STRING",
            "required": true
          }],
          "tql": "SELECT * FROM aws_athena.get_query_results\n  WHERE QueryExecutionId=@id\n\n/*\n * For sample code and reference material, visit\n * https://www.transposit.com/docs/get-started/sql-quickstart\n */"
        }
      }, {
        "id": "z_deprecated_check_status_and_post",
        "function": {
          "parameters": [{
            "name": "response_url",
            "type": "STRING"
          }],
          "js": "check_status_and_post.js"
        }
      }],
      "dataConnections": [{
        "ref": "slack_identify.raw",
        "overrides": {
          "swagger": {
            "oauthConfig": {
              "scope": "identity.basic"
            },
            "executionLimits": {
            }
          }
        },
        "title": "Slack",
        "description": "Used to identify you on Slack"
      }, {
        "ref": "slack_bot.raw",
        "overrides": {
          "swagger": {
            "oauthConfig": {
              "scope": "chat:write:bot"
            },
            "executionLimits": {
            }
          }
        }
      }, {
        "ref": "slack.raw"
      }, {
        "ref": "aws_athena.raw.aws_basic"
      }],
      "javascriptAPIVersion": "3",
      "source": "src",
      "userSettingsDisplay": "GENERATED",
      "appSettings": [{
        "name": "signupSheetId",
        "type": "STRING",
        "default": "1NMbjP6dPM4Db_CtL3Onxv68cS9_Y34QwLzxBsb6PHxs"
      }, {
        "name": "secondAppSheetId",
        "type": "STRING",
        "default": "1VFounIbuOOf961PF223FihLAWhS1JAnYQmLZ29b6etI"
      }, {
        "name": "activeAppSheetId",
        "type": "STRING",
        "default": "1ErmKnF_BpYXexA-w9NTyTf_StKtdWHPqr3hWCioYV3Q"
      }, {
        "name": "activeUserSheetId",
        "type": "STRING",
        "default": "1cdM1Dj12qQtDjY8n7J_UMkTYgMV7XIDB0Nf-wAhqOh0"
      }, {
        "type": "STRING"
      }]
    }
  }
}